    "@fxp/jquery-scroller": "github:fxpio/fxp-jquery-scroller#semver:^1.0.0",
    "@symfony/webpack-encore": "^1.1.0",
    "bootstrap": "^5.1.0",
    "file-loader": "^6.0.0",
